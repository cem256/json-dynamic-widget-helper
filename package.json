{
  "name": "json-dynamic-widget-helper",
  "displayName": "JSON Dynamic Widget Helper",
  "description": "Provides tools to manipulate JSON for `json_dynamic_widget` package",
  "version": "0.0.3",
  "publisher": "cem256",
  "repository": {
    "type": "git",
    "url": "https://github.com/cem256/json-dynamic-widget-helper"
  },
  "icon": "assets/icon/logo.png",
  "categories": [
    "Snippets",
    "Programming Languages"
  ],
  "keywords": [
    "json dynamic widget",
    "json dynamic widget helper",
    "dart",
    "flutter",
    "json_dynamic_widget",
    "json_dynamic_widget helper"
  ],
  "engines": {
    "vscode": "^1.93.0"
  },
  "activationEvents": [
    "onLanguage:json"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.11.1",
    "typescript": "^5.6.2"
  }
}
