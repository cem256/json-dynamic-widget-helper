{
  "name": "json-dynamic-widget-helper",
  "displayName": "json-dynamic-widget-helper",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "json-dynamic-widget-helper.showMenu",
        "title": "Show Widget Actions Menu"
      }
    ],
    "keybindings": [
      {
        "command": "json-dynamic-widget-helper.showMenu",
        "key": "cmd+.",
        "when": "editorLangId == json",
        "mac": "cmd+.",
        "linux": "ctrl+.",
        "win": "ctrl+."
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "json-dynamic-widget-helper.showMenu",
          "when": "resourceLangId == json",
          "group": "modification"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.94.0",
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.11.1",
    "typescript": "^5.6.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
